[user]
	name = Suwato
	email = takashi.sato@topgate.co.jp
[color]
	ui = auto
[core]
	quotepath = false
	excludesfile = /opt/boxen/config/git/gitignore
	precomposeunicode = true
[alias]
	graph = log --graph --all --pretty=format:'%Cgreen%h%Creset %cn %s %Cred%d%Creset'

[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /opt/homebrew-cask/Caskroom/sourcetree/1.9.7/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple

[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less

[hub]
protocol = https
[help]
	autocorrect = 1

[url "git@github.com:"]
    insteadOf = https://github.com/

